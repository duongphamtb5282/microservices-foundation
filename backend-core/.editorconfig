root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[*.java]
indent_style = space
indent_size = 2
max_line_length = 100

# Java import ordering (fallback for editors that support it)
ij_java_class_count_to_use_import_on_demand = 999
ij_java_names_count_to_use_import_on_demand = 999
ij_java_imports_layout = java.**, javax.**, |, *, |, com.demo.**, |, $*
ij_java_optimize_imports_on_the_fly = true
